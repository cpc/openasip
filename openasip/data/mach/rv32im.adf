      <bind name="3">in2</bind>
      <pipeline/>
    </ctrl-operation>
    <ctrl-operation>
      <name>bgeur</name>
      <bind name="1">pc</bind>
      <bind name="2">in</bind>
      <bind name="3">in2</bind>
      <pipeline/>
    </ctrl-operation>
    <address-space>instructions</address-space>
    <delay-slots>2</delay-slots>
    <guard-latency>1</guard-latency>
  </global-control-unit>

