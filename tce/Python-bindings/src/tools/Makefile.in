all: tools.main.cpp

clean::
	/bin/rm -f *.cpp *.hpp *.d *.md5.sum

%.main.cpp: %_bindings.pypp
	export PYTHONPATH="..:../../tools/pygccxml"; @PYTHON@ $<

%.d: %.pypp
	export PYTHONPATH="..:../../tools/pygccxml"; @PYTHON@ $< --generate-dependences 

